<TESTSCONFIGURATION>

<QTEST name="DeadChannels2D">
	 <TYPE>DeadChannel</TYPE>	 
	 <PARAM name="error">0.40</PARAM>
	 <PARAM name="warning">0.20</PARAM>
	 <PARAM name="threshold">3</PARAM>
         <PARAM name="useEmptyBins">1</PARAM>
</QTEST>

<QTEST name="HotChannels2D">
	 <TYPE>NoisyChannel</TYPE>   
	 <PARAM name="error">0.90</PARAM>
	 <PARAM name="warning">0.80</PARAM>
	 <PARAM name="tolerance">0.5</PARAM>
	 <PARAM name="neighbours">1</PARAM>
</QTEST>

<QTEST name="HotChannelsRate">
  <TYPE>NoisyChannel</TYPE>   
  <PARAM name="error">0.90</PARAM>
  <PARAM name="warning">0.80</PARAM>
  <PARAM name="tolerance">0.5</PARAM>
  <PARAM name="neighbours">5</PARAM>
</QTEST>

<LINK name="*RPCTF_muons_tower_phipacked*">
   <TestName activate="true">HotChannels2D</TestName> 
   <TestName activate="true">DeadChannels2D</TestName>
</LINK>

<LINK name="*RPCTF_rate*">
  <TestName activate="true">HotChannelsRate</TestName> 
</LINK>



</TESTSCONFIGURATION>
